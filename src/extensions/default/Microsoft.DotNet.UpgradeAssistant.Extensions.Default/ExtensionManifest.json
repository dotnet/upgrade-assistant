{
  "ExtensionName": "Default",

  "ConfigUpdater": {
    "ConfigFilePaths": [
      "app.config"
    ]
  },

  "TryConvert": {
    "ToolPath": "./try-convert/try-convert.dll",
    "Arguments": [
      "--no-backup",
      "--keep-current-tfms"
    ]
  },

  "PackageUpdater": {
    "PackageMapPath": "PackageMaps"
  },

  "ExtensionServiceProviders": [
    "Microsoft.DotNet.UpgradeAssistant.Extensions.Default.dll"
  ]
}